{
    "name": "ca-cams",
    "productName": "Caltrans Cameras",
    "description": "Lets you look at many different cameras at once",
    "keywords": [],
    "main": "./main.js",
    "version": "0.6.5",
    "author": "child-duckling",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "check-internet-connected": "^2.0.5",
        "discord-rpc": "^3.2.0",
        "electron-create-menu": "^3.0.1",
        "electron-dl": "^3.1.0",
        "electron-first-run": "^3.0.0",
        "electron-json-storage": "^4.3.0",
        "electron-prompt": "^1.6.1",
        "electron-settings": "^4.0.2",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^7.0.0",
        "jwt-decode": "^3.1.2",
        "keytar": "^7.6.0",
        "update-electron-app": "^2.0.1"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.54",
        "@electron-forge/maker-deb": "^6.0.0-beta.54",
        "@electron-forge/maker-rpm": "^6.0.0-beta.54",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
        "@electron-forge/maker-zip": "^6.0.0-beta.54",
        "electron": "^11.4.3"
    },
    "build": {
        "appId": "cacams",
        "protocols": {
            "name": "cal-cam-link",
            "schemes": [
                "cal-cam"
            ]
        },
        "mac": {
            "category": "misc"
        }
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./assets/icon.png"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ca_cams"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-dmg",
                    "config": {
                        "icon": "./assets/icon.png",
                        "format": "ULFO"
                    }
                }
            ]
        }
    }
}
